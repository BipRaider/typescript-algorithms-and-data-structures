{
  "name": "typescript-algorithms-and-data-structures",
  "version": "0.0.1",
  "description": "Algorithms and data-structures implemented on TypeScript",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src/*",
    "test": "jest",
    "ci": "npm run lint && npm run test -- --coverage",
    "start": "run-p esbuild dev",
    "dev": "wait-on ./dist/index.js && node --watch ./dist/index.js",
    "esbuild": "ts-node-dev --rs --poll --respawn -r tsconfig-paths/register ./esbuild.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BipRaider/typescript-algorithms-and-data-structures"
  },
  "keywords": [
    "computer-science",
    "cs",
    "data-structures",
    "algorithms",
    "javascript",
    "typescript",
    "algorithm",
    "javascript-algorithms",
    "typescript-algorithms",
    "sorting-algorithms",
    "graph",
    "tree"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BipRaider/typescript-algorithms-and-data-structures/issues"
  },
  "homepage": "https://github.com/BipRaider/typescript-algorithms-and-data-structures#readme",
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "wait-on": "^7.0.1"
  }
}
